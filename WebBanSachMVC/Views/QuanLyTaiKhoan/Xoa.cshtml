@model WebBanSachMVC.Models.Admin

@{
    ViewBag.Title = "Xoa";
    Layout = "~/Views/Layout/AdminQuanLy.cshtml";
}

<link href="~/AdminCss/bootstrap.min.css" rel="stylesheet" />
<link href="~/AdminCss/metisMenu.min.css" rel="stylesheet" />
<link href="~/AdminCss/sb-admin-2.css" rel="stylesheet" />

<link href="~/AdminCss/font-awesome.min.css" rel="stylesheet" />
<div id="page-wrapper">
    <h3>Bạn có muốn xóa tài khoản này?</h3>
  

    <div class="form-group">
        <label for="inputPassword3" class="col-sm-4 control-label">Mã tài khoản</label>
        <div class="col-md-10">
            @Html.TextBoxFor(model => model.MaAdmin, null, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.MaAdmin)
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-4 control-label">Tên đăng nhập</label>
        <div class="col-md-10">
            @Html.TextBoxFor(model => model.TenDN, null, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.TenDN)
        </div>
    </div>

    <div class="form-group">
        <label for="inputPassword3" class="col-sm-4 control-label">Mật khẩu</label>
        <div class="col-md-10">
            @Html.TextBoxFor(model => model.MatKhau, null, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.MatKhau) <br />
        </div>
    </div>
   
   
    <div class="form-group" >
        <div class="col-md-10">
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Xóa" class="btn btn-default" /> |
                    @Html.ActionLink("Trở về", "QLDSTaiKhoanAdmin")
                </div>
            }

            </div>
        </div>

            <script src="~/AdminJS/jquery.min.js"></script>
            <script src="~/AdminJS/bootstrap.min.js"></script>
            <script src="~/AdminJS/metisMenu.min.js"></script>
            <script src="~/AdminJS/sb-admin-2.js"></script>
</div>